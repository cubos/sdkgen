{
  "name": "@cubos/api",
  "version": "0.0",
  "scripts": {
    "build": "json -I -f package.json -e 'this.name=\"@cubos/'$(basename `git rev-parse --show-toplevel`)'\"'; npm version 0.0.0; npm version 1.0.$(git rev-list --count HEAD); tsc; babel api.js -o api.js"
  },
  "dependencies": {
    "@types/ua-parser-js": "^0.7.30",
    "babel-runtime": "^6.23.0",
    "moment": "^2.17.1",
    "ua-parser-js": "^0.7.12"
  },
  "main": "api.js",
  "types": "api.d.ts",
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "babel-preset-es2015",
      "babel-preset-es2016",
      "babel-preset-es2017"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-es2017": "^6.22.0",
    "json": "^9.0.4",
    "typescript": "^2.2.1"
  }
}
